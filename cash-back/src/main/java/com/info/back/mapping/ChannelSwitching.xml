<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.info.back.dao.IChannelSwitchingDao">


	<select id="getChannelValue" parameterType="String" resultType="com.info.web.pojo.ChannelSwitching">
		SELECT description,channel_value AS channelValue FROM channel_switching WHERE description =#{description}
	</select>
	
</mapper>